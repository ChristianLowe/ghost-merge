script/source = "extends Node2D

onready var sprite = get_parent().get_node(\"Sprite\")

func _ready():
	modulate.a = 0.2

func _process(delta):
	update()

func _draw():
	var shadow_position = position + Vector2(0, 24)
	var radius = 12 + (sprite.current_height * .15)
	
	draw_circle(shadow_position, radius, Color.black)
	"

