script/source = "extends AnimatedSprite

var current_angle = 0.0
var current_height = 0.0
var variance = 12
var speed = 2

func _process(delta):
	current_angle += delta * speed
	current_height = sin(current_angle) * variance
	position = Vector2(0, current_height)
"

